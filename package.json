  "scripts": {
    "start": "craco start",
    "start:mock": "BROWSER=none MOCK=true craco start",
    "start:proxyServer": "nodemon --config nodemon.mock.json mock/index.js",
    "start:proxy": "concurrently -n mock,fe \"npm:start:proxyServer\" \"npm:start:mock\"",
    "build": "craco build",
    "build:analyze": "ANALYZE=true craco build",
    "lint": "eslint --ext js,jsx,ts,tsx ./src",
    "lint:fix": "eslint --ext js,jsx,ts,tsx --fix ./src",
    "test": "craco test",
    "eject": "react-scripts eject",
    "fix:svg": "svgo -f ./src -r --enable=prefixIds --disable=removeViewBox"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
